global function double tsat_eng (double p);double t, rhol, rhov

IF p = 0 THEN
	SetNull(p)
	RETURN p
END IF

p	= psi2mpa(p)

tsat( p, t, rhol, rhov )

RETURN kelvin2fahr(t)
end function


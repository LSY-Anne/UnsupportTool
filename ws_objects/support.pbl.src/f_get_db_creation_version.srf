global subroutine f_get_db_creation_version (ref string as_db_creation_version);SELECT version 
  INTO :as_DB_Creation_Version 
  FROM sys.syshistory 
 WHERE ( operation = 'INIT' ) 
USING SQLCA ;


end subroutine

